{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"svelteStrictMode": true,
	"bracketSpacing": true,
	"svelteBracketNewLine": false,
	"svelteSortOrder": "scripts-options-styles-markup"
}
